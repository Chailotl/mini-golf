<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.raus.miniGolf</groupId>
	<artifactId>mini-golf</artifactId>
	<version>v1.0</version>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<repositories>
		<repository>
			<id>jitpack.io</id>
			<url>https://jitpack.io</url>
		</repository>
	</repositories>
	<dependencies>
		<dependency>
			<groupId>com.github.rausy</groupId>
			<artifactId>short-utils</artifactId>
			<version>v1.0</version>
		</dependency>
		<dependency>
			<groupId>com.github.rausy</groupId>
			<artifactId>craft-lib</artifactId>
			<version>v1.0</version>
		</dependency>
	</dependencies>
</project>